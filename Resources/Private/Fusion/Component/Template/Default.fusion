prototype(Kleisli.Kubbar:Component.Template.Default) < prototype(Neos.Fusion:Component) {

    content = ''

    renderer = Neos.Fusion:Join {
        header = Kleisli.Kubbar:Component.Organism.Header
        main = Kleisli.Kubbar:Component.Organism.Main {
            content = ${props.content}
        }
        footer = Kleisli.Kubbar:Component.Organism.Footer
    }
}
